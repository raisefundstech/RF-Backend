{
  "compilerOptions": {
    "target": "ES2020",
    "experimentalDecorators": true,
    "module": "commonjs",
    "strict": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./build",
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "lib": ["es2015", "es2016", "esnext.asynciterable", "es2020"]
  }
}
  